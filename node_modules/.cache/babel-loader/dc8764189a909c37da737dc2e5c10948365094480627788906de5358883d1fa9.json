{"ast":null,"code":"import ProductService from \"../service/ProductService\";\nimport PhotoService from \"../service/PhotoService\";\nexport default {\n  data: function data() {\n    return {\n      products: [],\n      images: [],\n      galleriaResponsiveOptions: [{\n        breakpoint: \"1024px\",\n        numVisible: 5\n      }, {\n        breakpoint: \"960px\",\n        numVisible: 4\n      }, {\n        breakpoint: \"768px\",\n        numVisible: 3\n      }, {\n        breakpoint: \"560px\",\n        numVisible: 1\n      }],\n      carouselResponsiveOptions: [{\n        breakpoint: \"1024px\",\n        numVisible: 3,\n        numScroll: 3\n      }, {\n        breakpoint: \"768px\",\n        numVisible: 2,\n        numScroll: 2\n      }, {\n        breakpoint: \"560px\",\n        numVisible: 1,\n        numScroll: 1\n      }]\n    };\n  },\n  created: function created() {\n    this.productService = new ProductService();\n    this.photoService = new PhotoService();\n  },\n  mounted: function mounted() {\n    var _this = this;\n    this.productService.getProductsSmall().then(function (products) {\n      _this.products = products;\n    });\n    this.photoService.getImages().then(function (images) {\n      _this.images = images;\n    });\n  }\n};","map":{"version":3,"mappings":"AAyDA,OAAOA,cAAa,MAAO,2BAA2B;AACtD,OAAOC,YAAW,MAAO,yBAAyB;AAClD,eAAe;EACXC,IAAI,kBAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE,EAAE;MACVC,yBAAyB,EAAE,CACvB;QACIC,UAAU,EAAE,QAAQ;QACpBC,UAAU,EAAE;MAChB,CAAC,EACD;QACID,UAAU,EAAE,OAAO;QACnBC,UAAU,EAAE;MAChB,CAAC,EACD;QACID,UAAU,EAAE,OAAO;QACnBC,UAAU,EAAE;MAChB,CAAC,EACD;QACID,UAAU,EAAE,OAAO;QACnBC,UAAU,EAAE;MAChB,CAAC,CACJ;MACDC,yBAAyB,EAAE,CACvB;QACIF,UAAU,EAAE,QAAQ;QACpBC,UAAU,EAAE,CAAC;QACbE,SAAS,EAAE;MACf,CAAC,EACD;QACIH,UAAU,EAAE,OAAO;QACnBC,UAAU,EAAE,CAAC;QACbE,SAAS,EAAE;MACf,CAAC,EACD;QACIH,UAAU,EAAE,OAAO;QACnBC,UAAU,EAAE,CAAC;QACbE,SAAS,EAAE;MACf,CAAC;IAET,CAAC;EACL,CAAC;EACDC,OAAO,qBAAG;IACN,IAAI,CAACC,cAAa,GAAI,IAAIX,cAAc,EAAE;IAC1C,IAAI,CAACY,YAAW,GAAI,IAAIX,YAAY,EAAE;EAC1C,CAAC;EACDY,OAAO,qBAAG;IAAA;IACN,IAAI,CAACF,cAAc,CAACG,gBAAgB,EAAE,CAACC,IAAI,CAAC,UAACZ,QAAQ,EAAK;MACtD,KAAI,CAACA,QAAO,GAAIA,QAAQ;IAC5B,CAAC,CAAC;IACF,IAAI,CAACS,YAAY,CAACI,SAAS,EAAE,CAACD,IAAI,CAAC,UAACX,MAAM,EAAK;MAC3C,KAAI,CAACA,MAAK,GAAIA,MAAM;IACxB,CAAC,CAAC;EACN;AACJ,CAAC","names":["ProductService","PhotoService","data","products","images","galleriaResponsiveOptions","breakpoint","numVisible","carouselResponsiveOptions","numScroll","created","productService","photoService","mounted","getProductsSmall","then","getImages"],"sourceRoot":"","sources":["C:\\Users\\eletr\\Desktop\\AtlantisVue\\src\\components\\MediaDemo.vue"],"sourcesContent":["<template>\n    <div class=\"grid p-fluid\">\n        <div class=\"col-12\">\n            <div class=\"card\">\n                <h5>Carousel</h5>\n                <Carousel :value=\"products\" :numVisible=\"3\" :numScroll=\"3\" :circular=\"false\" :responsiveOptions=\"carouselResponsiveOptions\">\n                    <template #item=\"product\">\n                        <div class=\"product-item\">\n                            <div class=\"product-item-content\">\n                                <div class=\"mb-3\">\n                                    <img :src=\"'demo/images/product/' + product.data.image\" :alt=\"product.data.name\" class=\"product-image\" />\n                                </div>\n                                <div>\n                                    <h4 class=\"mb-1\">\n                                        {{ product.data.name }}\n                                    </h4>\n                                    <h6 class=\"mt-0 mb-3\">${{ product.data.price }}</h6>\n                                    <span :class=\"'product-badge status-' + product.data.inventoryStatus.toLowerCase()\">{{ product.data.inventoryStatus }}</span>\n                                    <div class=\"car-buttons mt-5\">\n                                        <Button type=\"button\" class=\"p-button p-button-rounded mr-2\" icon=\"pi pi-search\"></Button>\n                                        <Button type=\"button\" class=\"p-button-success p-button-rounded mr-2\" icon=\"pi pi-star-fill\"></Button>\n                                        <Button type=\"button\" class=\"p-button-help p-button-rounded\" icon=\"pi pi-cog\"></Button>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                    </template>\n                </Carousel>\n            </div>\n        </div>\n\n        <div class=\"col-12\">\n            <div class=\"card\">\n                <h5>Galleria</h5>\n                <Galleria :value=\"images\" :responsiveOptions=\"galleriaResponsiveOptions\" :numVisible=\"7\" :circular=\"true\" containerStyle=\"max-width: 800px; margin: auto\">\n                    <template #item=\"slotProps\">\n                        <img :src=\"slotProps.item.itemImageSrc\" :alt=\"slotProps.item.alt\" style=\"width: 100%; display: block\" />\n                    </template>\n                    <template #thumbnail=\"slotProps\">\n                        <img :src=\"slotProps.item.thumbnailImageSrc\" :alt=\"slotProps.item.alt\" tyle=\"width: 100%; display: block;\" />\n                    </template>\n                </Galleria>\n            </div>\n        </div>\n\n        <div class=\"col-12\">\n            <div class=\"card\">\n                <h5>Image</h5>\n                <div class=\"flex justify-content-center\">\n                    <Image src=\"demo/images/galleria/galleria11.jpg\" alt=\"Image\" width=\"250\" preview />\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport ProductService from \"../service/ProductService\";\nimport PhotoService from \"../service/PhotoService\";\nexport default {\n    data() {\n        return {\n            products: [],\n            images: [],\n            galleriaResponsiveOptions: [\n                {\n                    breakpoint: \"1024px\",\n                    numVisible: 5,\n                },\n                {\n                    breakpoint: \"960px\",\n                    numVisible: 4,\n                },\n                {\n                    breakpoint: \"768px\",\n                    numVisible: 3,\n                },\n                {\n                    breakpoint: \"560px\",\n                    numVisible: 1,\n                },\n            ],\n            carouselResponsiveOptions: [\n                {\n                    breakpoint: \"1024px\",\n                    numVisible: 3,\n                    numScroll: 3,\n                },\n                {\n                    breakpoint: \"768px\",\n                    numVisible: 2,\n                    numScroll: 2,\n                },\n                {\n                    breakpoint: \"560px\",\n                    numVisible: 1,\n                    numScroll: 1,\n                },\n            ],\n        };\n    },\n    created() {\n        this.productService = new ProductService();\n        this.photoService = new PhotoService();\n    },\n    mounted() {\n        this.productService.getProductsSmall().then((products) => {\n            this.products = products;\n        });\n        this.photoService.getImages().then((images) => {\n            this.images = images;\n        });\n    },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.product-item {\n    .product-item-content {\n        border: 1px solid var(--surface-d);\n        border-radius: 3px;\n        margin: 0.3rem;\n        text-align: center;\n        padding: 2rem 0;\n    }\n    .product-image {\n        width: 50%;\n        box-shadow: 0 3px 6px rgba(0, 0, 0, 0.16), 0 3px 6px rgba(0, 0, 0, 0.23);\n    }\n}\n\n.product-badge {\n\tborder-radius: 2px;\n\tpadding: .25em .5rem;\n\ttext-transform: uppercase;\n\tfont-weight: 700;\n\tfont-size: 12px;\n\tletter-spacing: .3px;\n\t&.status-instock {\n\t\tbackground: #C8E6C9;\n\t\tcolor: #256029;\n\t}\n\t&.status-outofstock {\n\t\tbackground: #FFCDD2;\n\t\tcolor: #C63737;\n\t}\n\t&.status-lowstock {\n\t\tbackground: #FEEDAF;\n\t\tcolor: #8A5340;\n\t}\n}\n</style> \n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}